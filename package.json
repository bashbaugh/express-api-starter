{
  "name": "hackclub-lab-api",
  "version": "0.0.1",
  "description": "API for the Hack Club Lab",
  "main": "dist/index.js",
  "repository": "https://github.com/bashbaugh/lab-api.git",
  "author": "Benjamin Ashbaugh <hi@benjaminashbaugh.me>",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/index.ts",
    "start": "NODE_ENV=\"production\" node dist/index.js",
    "format": "prettier --write \"src/**/*.{js,ts,ts}\" --config ./prettier.config.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "prettier": "^2.5.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  }
}
